#include<stdio.h>
using namespace std;
int main(){
    
    // graph representation by adj list for undirected graph
    int n,m;
    cin>>n>>m;
    vector<int> adj[n+1];
    for(int i=0;i<m;i++){
        int u,v;
        cin>>u>>v;
        adj[u].push_back(v);
        adj[v].push_back(u);
    }
        
    
    return 0;
}






#include<stdio.h>
using namespace std;
int main(){
    
    // graph representation by adj list for directed graph
    int n,m;
    cin>>n>>m;
    vector<int> adj[n+1];
    for(int i=0;i<m;i++){
        //u---->v
        int u,v;
        cin>>u>>v;
        adj[u].push_back(v);
        //adj[v].push_back(u);
    }
        
    
    return 0;
}
